{
  "version": 2,
  "name": "Whale Scout",
  "alias": "whalescout.org",
  "builds": [{ "src": "next.config.js", "use": "@now/next" }],
  "routes": [
    {
      "src": "/feed/podcast/",
      "dest": "https://admin.whalescout.org/feed/podcast/"
    },
    {
      "src": "/feed/podcast",
      "dest": "https://admin.whalescout.org/feed/podcast"
    },
    {
      "src": "/media",
      "dest": "https://audio.whalescout.org/media"
    },
    {
      "src": "/media/",
      "dest": "https://audio.whalescout.org/media/"
    }
  ]
}
